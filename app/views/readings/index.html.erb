<h1>Listing readings</h1>

<table>
  <tr>
    <th>Job</th>
    <th>Patch</th>
    <th>Spectral 1</th>
    <th>Spectral 2</th>
    <th>Spectral 3</th>
    <th>Spectral 4</th>
    <th>Rgb r</th>
    <th>Rgb g</th>
    <th>Rgb b</th>
  </tr>

<% @readings.each do |reading| %>
  <tr>
    <td><%=h reading.job_id %></td>
    <td><%=h reading.patch_id %></td>
    <td><%=h reading.spectral_1 %></td>
    <td><%=h reading.spectral_2 %></td>
    <td><%=h reading.spectral_3 %></td>
    <td><%=h reading.spectral_4 %></td>
    <td><%=h reading.rgb_r %></td>
    <td><%=h reading.rgb_g %></td>
    <td><%=h reading.rgb_b %></td>
    <td><%= link_to 'Show', reading %></td>
    <td><%= link_to 'Edit', edit_reading_path(reading) %></td>
    <td><%= link_to 'Destroy', reading, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New reading', new_reading_path %>